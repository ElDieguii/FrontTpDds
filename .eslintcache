Open(false);\n    };\n\n    const handleRegister = (event) => {\n        event.preventDefault()\n        if(event.target[1].value!==event.target[2].value){\n            setMessage('Las contraseñas no coinciden')\n            setOpen(true)  \n        }else{\n            setLoading(true)\n            const datosUsuario={\n                nombreDeUsuario: event.target[0].value,\n                contrasenia: event.target[1].value,\n                esAdmin:switched\n            }\n    \n            const url = `http://localhost:8080/auth/registro`\n\n            axios.post(url,datosUsuario)\n            .then((res) => {\n                setLoading(false)\n                setMessage('Registro Exitoso')\n                setOpen(true)\n                setError(false)\n            })\n            .catch((err) => {\n                setLoading(false)\n                setMessage(err.response ? err.response.data.error : \"Error de conexion\\n\"+err)\n                setError(true)\n                setOpen(true)  \n            })\n        }\n    }\n\n      const handleChange = (event) => {\n          setSwitched(event.target.checked)\n      };\n    return (\n        \n        <div className=\"container\">\n            <form onSubmit={handleRegister}>\n                <h1 className={'title'}>Se uno mas de nosotros</h1>\n                <Input placeholder='Ingresar usuario'/>\n                <Input \n                    placeholder='Ingresar contraseña'\n                    type='Password'\n                />\n                <Input \n                    placeholder='Repetir contraseña'\n                    type='Password'\n                />\n                <div className={'adminSwitch'}>\n                <FormControlLabel\n                    control={\n                    <Switch\n                        checked={switched}\n                        onChange={handleChange}\n                        name=\"checkedB\"\n                        color=\"primary\"\n                    />\n                    }\n                    label=\"Es Administrador\"\n                />\n                </div>\n                {loading ?\n                        <CircularProgress className=\"loading\"/> \n                : \n                    <Button\n                        name='Registrarse'\n                    />\n                }\n            </form>\n            <Snackbar open={open} autoHideDuration={3000} onClose={handleClose}>\n                <Alert onClose={handleClose} severity={error? \"error\":\"success\"}>\n                    {message}\n                </Alert>\n            </Snackbar>\n\n        </div>\n    )\n} ","/Users/diegovivona/Desktop/Facultad/FrontTpDds/src/components/Products/Products.js",[],"/Users/diegovivona/Desktop/Facultad/FrontTpDds/src/components/SideBar/SideBarData.js",["83","84","85"],"import React, {useState} from 'react'\nimport * as FaIcons from 'react-icons/fa'\nimport * as AiIcons from 'react-icons/ai'\nimport * as IoIcons from 'react-icons/io'\nimport * as BiIcons from 'react-icons/bi'\nimport * as GiIcons from 'react-icons/gi'\n\n\nexport const SideBarData = [\n    {\n        title: 'Home',\n        path: '/',\n        icon: <AiIcons.AiFillHome />,\n        cName: 'nav-text'\n    },\n    {\n        title: 'Login',\n        path: '/login',\n        icon: <BiIcons.BiLogIn />,\n        cName: 'nav-text'\n    },\n    {\n        title: 'Products',\n        path: '/products',\n        icon: <BiIcons.BiShoppingBag />,\n        cName: 'nav-text'\n    },\n    {\n        title: 'Register',\n        path: '/register',\n        icon: <IoIcons.IoIosPersonAdd />,\n        cName: 'nav-text'\n    }\n]","/Users/diegovivona/Desktop/Facultad/FrontTpDds/src/components/Input/Input.js",[],"/Users/diegovivona/Desktop/Facultad/FrontTpDds/src/components/Button/Button.js",["86"],"import styled, {keyframes} from 'styled-components';\nimport './Button.css'\n\nconst pulse = keyframes`\n  0%{\n    transform: scale(1);\n  }\n\n  100%{\n    tr